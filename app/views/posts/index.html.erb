<h1>Posts</h1>

<table class="table">
	<thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
    </tr>
	</thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr><td><a href="<%= post_path(post.id) %>"><%= post.title %></a></td><td><%= post.author %></td></tr>
    <% end %>
  </tbody>
</table>